<!-- FOOD TRUCK MANAGER NAVIGATION -->

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_foodtruck_manager"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_spot_calendar"
        android:name="io.usys.report.basicUser.calendar.FoodCalendarFragment"
        android:label="@string/title_calendar"
        tools:layout="@layout/spot_calendar_fragment" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="io.usys.report.ui.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_cart"
        android:name="io.usys.report.basicUser.calendar.FoodCalendarFragment"
        android:label="@string/title_orgs"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_org_list"
        android:name="io.usys.report.ui.OrganizationListFragment"
        android:label="@string/title_orgs"
        tools:layout="@layout/fragment_org_list">
    </fragment>

    <fragment
        android:id="@+id/navigation_coaches_list"
        android:name="io.usys.report.ui.CoachListFragment"
        android:label="@string/title_coaches"
        tools:layout="@layout/fragment_coaches_list">
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="io.usys.report.ui.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_user_profile" />

    <fragment
        android:id="@+id/navigation_base_frag"
        android:name="io.usys.report.ui.YsrFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_org_list">
    </fragment>

</navigation>